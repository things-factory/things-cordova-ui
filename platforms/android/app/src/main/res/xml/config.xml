<?xml version='1.0' encoding='utf-8'?>
<widget id="com.things.factory.dd" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SSDP">
        <param name="android-package" value="com.things.factory.ssdp.SSDPLauncher" />
    </feature>
    <name>DeviceDiscover</name>
    <description>
    </description>
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="./assets/images/hatiolab-logo.png" />
    <icon density="xhdpi" src="./assets/images/hatiolab-logo.png" />
    <icon density="xxhdpi" src="./assets/images/hatiolab-logo.png" />
    <icon density="xxxhdpi" src="./assets/images/hatiolab-logo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
